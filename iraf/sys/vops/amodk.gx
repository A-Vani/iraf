# AMODK -- Compute the modulus of a vector by a constant (generic).

procedure amodk$t (a, b, c, npix)

PIXEL	a[ARB]
PIXEL	b
PIXEL	c[ARB]
size_t	npix, i
$if (datatype == sil)
PIXEL	$tmod()
$endif

begin
$if (datatype == sil)
	do i = 1, npix
	    c[i] = $tmod (a[i], b)
$else
	do i = 1, npix
	    c[i] = mod (a[i], b)
$endif
end
