
all:	iraf.h mach.h mkpkg.inc

iraf.h::
	rm -f iraf.h
	cpp -traditional-cpp -undef -P $(HSI_CF) $(DEFS) iraf.h.in | tr ';' '#' > iraf.h

mach.h::
	rm -f mach.h
	cpp -traditional-cpp -undef -P $(HSI_CF) $(DEFS) mach.h.in | tr ';' '#' > mach.h

mkpkg.inc::
	rm -f mkpkg.inc
	cat mkpkg.inc.in | sed -e "s|@LFLAGS@|$(XC_LFLAGS)|" > mkpkg.inc

clean::
	rm -f iraf.h mach.h mkpkg.inc
