$if (datatype == r)
include "nlfitdefr.h"
$else
include "nlfitdefd.h"
$endif

# NLPGET - Retreive parameter values

procedure nlpget$t (nl, params, nparams)

pointer	nl			# pointer to the nlfit structure
PIXEL	params[ARB]		# parameter array
size_t	nparams			# the number of the parameters

size_t	sz_val

begin
	nparams = NL_NPARAMS(nl)
	sz_val = nparams
	call amov$t (PARAM(NL_PARAM(nl)), params, sz_val)
end
